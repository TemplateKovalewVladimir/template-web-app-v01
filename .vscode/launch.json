{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}",
      "args": [
        "app:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8101",
        "--proxy-headers",
        "--forwarded-allow-ips=*"
      ],
      "jinja": true,
      "justMyCode": true
    }
  ]
}